<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/catFragment">

    <fragment
        android:id="@+id/catFragment"
        android:name="com.dxshulya.catapi.ui.CatFragment"
        android:label="news_fragment"
        tools:layout="@layout/cat_list" >
        <action
            android:id="@+id/action_catFragment_to_showCatFragment"
            app:destination="@id/showCatFragment" />
    </fragment>
    <fragment
        android:id="@+id/showCatFragment"
        android:name="com.dxshulya.catapi.ui.ShowCatFragment"
        android:label="show_cat_fragment"
        tools:layout="@layout/show_cat" >
        <argument
            android:name="catUrl"
            app:argType="string"
            app:nullable="false"/>
    </fragment>
</navigation>